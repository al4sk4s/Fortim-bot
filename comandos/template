from twitchio.ext import commands

class NomeDoCog(commands.Cog):
    def __init__(self, bot):
        self.bot = bot

    @commands.command(name="comando")
    async def comando(self, ctx):
        await ctx.send("Executou o comando com sucesso!")

def prepare(bot):
    bot.add_cog(NomeDoCog(bot))
